 {
    "name": "Welcome to the world of extensions",
    "version": "1.0",
    "description": "Welcome here it is to build!",
    "permissions": ["activeTab","declarativeContent","storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
      "options_page": "options.html",
     
     "page_action": {
      "default_popup": "popup.html",
       "default_icon": {
        "16": "images/notebook.png",
        "32": "images/notebook.png",
        "48": "images/notebook.png",
        "128": "images/notebook.png"
      }
     },
     "icons": {
      "16": "images/notebook.png",
      "32": "images/notebook.png",
      "48": "images/notebook.png",
      "128": "images/notebook.png"
    },
    "manifest_version": 2 
  }